{
  "agent_name": "Tester",
  "report": "http://10.235.165.6:8000/perfmon/post",
  "perfmon": [
    {
      "name": "cpu_0_freq",
      "type": "cpufreq",
      "delay": 60,
      "wait": 10,
      "tasks": {
        "method": "readfile",
        "path": "/sys/devices/system/cpu/cpufreq/policy0/scaling_cur_freq",
        "format": "ToInt",
        "expect": "int",
        "length": 1024,
        "timeout": 10
      }
    },
    {
      "name": "cpu_0_temp",
      "type": "cputemp",
      "delay": 60,
      "tasks": {
        "method": "readfile",
        "path": "/.../",
        "format": "ToInt",
        "expect": "int",
        "timeout": 10
      }
    },
    {
      "name": "cpu_cores",
      "type": "invalid",
      "delay": 60,
      "tasks": {
        "method": "execute",
        "exec": "cat",
        "params": [
          "/proc/cpuinfo"
        ],
        "format": null,
        "expect": "string",
        "timeout": 10
      }
    }
  ]
}
